{
  "name": "Photo",
  "isPublic": true,
  "locals": {
    "background_color": "#fff",
    "text_color": "rgba(0, 0, 0, 0.85)",
    "link_color": "#00008B",
    "hide_dates": false,
    "infinite_scroll": true,
    "collapse_menu_by_default": false,
    "number_of_rows": 7,
    "number_of_rows_range": [
      1,
      20
    ],
    "thumbnails_per_row": 6,
    "thumbnails_per_row_range": [
      1,
      7
    ],
    "page_size": 42,
    "date_display": "MMMM D, Y",
    "body_font": {},
    "spacing": 2,
    "spacing_range": [0, 5],
    "grid_layout": "square",
    "grid_layout_options": [
      "ragged",
      "square"
    ]
  },
  "views": {
    "tagged.html": {
      "partials": {
        "title": "{{{tag}}} - {{{title}}}",
        "description": "All of the entries posted on {{{title}}} tagged {{{tag}}}"
      }
    },
    "archives.html": {
      "url": "/archives",
      "partials": {
        "title": "Archives - {{{title}}}",
        "description": "All the entries posted on {{{title}}}"
      }
    },
    "entries.html": {
      "partials": {
        "title": "{{{title}}}",
        "description": "The latest entries posted on {{{title}}}"
      }
    },
    "entry.html": {
      "partials": {
        "title": "{{{entry.title}}} - {{{title}}}",
        "description": "{{{entry.summary}}}"
      }
    },
    "public.html": {
      "partials": {
        "title": "Index of {{{path}}}",
        "description": "All the files and folders inside {{{path}}}"
      }
    }
  }
}
